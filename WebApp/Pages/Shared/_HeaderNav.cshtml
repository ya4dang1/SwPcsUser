@using Microsoft.AspNetCore.Identity
@inject SignInManager<ApplicationUser> SignInManager
@inject UserManager<ApplicationUser> UserManager

<header class="app-header navbar">
  <a class="navbar-brand" href="#" style="">
    <img class="navbar-brand-full" src="data:image/png;base64," height="45" alt="Logo">
    <img class="navbar-brand-minimized" src="" height="40" alt="Logo">
  </a>

  <ul class="nav navbar-nav mr-auto d-lg-none">
    <li class="nav-item dropdown">
      <a class="nav-link nav-link" Title="Menu" data-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false">
        <span class="navbar-toggler-icon"></span>
      </a>
      <div class="dropdown-menu dropdown-menu-left">
        <a class="dropdown-item" asp-page="/Card/Index">
          <i class="far fa-credit-card"></i> @Localizer["All Cards"]
        </a>
        <a class="dropdown-item" asp-page="/User/UpdateProfile">
          <i class="fa fa-user-edit"></i> @Localizer["Update Profile"]
        </a>
        <a class="dropdown-item" asp-area="Identity" asp-page="/Account/Manage/ChangePassword">
          <i class="fa fa-lock"></i> @Localizer["Change Password"]
        </a>
      </div>
    </li>
  </ul>

  <ul class="nav navbar-nav d-md-down-none">
    <li class="nav-item px-3">
      <a class="nav-link" asp-page="/Card/Index"><strong>@Localizer["All Cards"]</strong></a>
    </li>
    <li class="nav-item px-3">
      <a class="nav-link" asp-page="/User/UpdateProfile"><strong>@Localizer["Update Profile"]</strong></a>
    </li>
    <li class="nav-item px-3">
      <a class="nav-link" asp-area="Identity" asp-page="/Account/Manage/ChangePassword"><strong>@Localizer["Change Password"]</strong></a>
    </li>
  </ul>

  <div>
    <div class="mr-0 float-right" style="line-height: 35px">
      <partial name="_SelectLanguagePartial" />
    </div>
    <div class="mr-0 float-right">
      <form method="post" novalidate="novalidate">
        <span class="d-sm-down-none">@Localizer["Hello"] <b>@User.Identity.Name</b>!</span>
        <button type="submit" class="btn" asp-area="Identity" asp-page="/Account/Logout" asp-route-toast="logout" title="Lockout">
          <i class="fas fa-sign-out-alt"></i>
        </button>
      </form>
    </div>
  </div>
  <button class="navbar-toggler aside-menu-toggler d-md-down-none d-none" type="button" data-toggle="aside-menu-lg-show">
    <span class="navbar-toggler-icon"></span>
  </button>
  <button class="navbar-toggler aside-menu-toggler d-lg-none d-none" type="button" data-toggle="aside-menu-show">
    <span class="navbar-toggler-icon"></span>
  </button>
</header>
